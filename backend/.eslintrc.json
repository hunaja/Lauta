{
    "env": {
        "es6": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "eslint"
    ],
    "parser": "@babel/eslint-parser",
    "rules": {
        "import/extensions": 0,
        "no-underscore-dangle": 0,
        "consistent-return": 0,
        "indent": ["error", 4],
        "default-param-last": 0,
        "no-param-reassign": 0,
        "quotes": ["error", "double"],
        "operator-assignment": 0
    },
    "parserOptions": {
        "requireConfigFile": false
    }
}
